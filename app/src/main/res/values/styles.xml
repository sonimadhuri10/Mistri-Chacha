<resources>

    <style name="AppSearchView" parent="Widget.AppCompat.SearchView" >
        <item name="android:textSize">14dp</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemenew" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="AppTheme1" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="AppThemesearch" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorWhite</item>
    </style>

    <style name="text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="price_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#008000</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="light_text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#8F8E8E</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="grey_text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#A5A0A0</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="dark_text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">7dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="text_color_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="Header_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15dp</item>
        <item name="android:fontFamily">sans-serif-condensed-medium</item>
    </style>

    <style name="button_text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="button_color_text_style">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="AppTheme1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />

</resources>